{
  "name": "scansnap-lark-sync",
  "version": "1.0.0",
  "description": "ScanSnap iX2500 to Lark Base business card data sync tool",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "scansnap-lark-sync": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/cli.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts --ignore-pattern '**/*.test.ts'",
    "lint:fix": "eslint src --ext .ts --ignore-pattern '**/*.test.ts' --fix",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "scansnap",
    "lark",
    "business-card",
    "sync"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.5",
    "commander": "^11.1.0",
    "csv-parse": "^5.5.3",
    "dotenv": "^16.3.1",
    "form-data": "^4.0.0",
    "iconv-lite": "^0.6.3"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "@vitest/coverage-v8": "^1.2.0",
    "eslint": "^8.56.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  }
}
